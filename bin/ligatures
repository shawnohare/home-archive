#!/usr/bin/env bash
echo "Printing ligatures."
ligs=(
  'www'
  '<*'
  '<*>'
  '<+>'
  '<$>'
  '***'
  '<|'
  '|>'
  '<|>'
  '!!'
  '||'
  '==='
  '==>'
  '<<<'
  '>>>'
  '<>'
  '+++'
  '<-'
  '->'
  '=>'
  '>>'
  '<<'
  '>>='
  '=<<'
  '..'
  '...'
  '::'
  '-<'
  '>-'
  '-<<'
  '>>-'
  '++'
  '/='
  '=='
  '!='
)
for lig in "${ligs[@]}"; do
    echo "${lig}"
done

